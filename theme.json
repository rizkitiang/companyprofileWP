{
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"customTemplates": [
		{
			"name": "page-with-sidebar",
			"title": "Page (With Sidebar)",
			"postTypes": [
				"post",
				"page"
			]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#161D25",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#1675ED",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#E0FB60",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#fff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#161D25",
					"name": "Links",
					"slug": "custom-links"
				},
				{
					"color": "#161D25",
					"name": "Links Hover",
					"slug": "custom-links-hover"
				},
				{
					"color": "#262626",
					"name": "Text",
					"slug": "text-primary"
				},
				{
					"color": "#181818",
					"name": "Footer",
					"slug": "footer"
				},
				{
					"color": "#ffffff29",
					"name": "white16",
					"slug": "white16"
				},
				{
					"name": "Header Image overlay",
					"color": "transparent",
					"slug": "header-image-overlay"
				},
				{
					"color": "#fff",
					"name": "Background",
					"slug": "background-primary"
				}
			],
			"duotone": [],
			"customDuotone": false,
			"defaultDuotone": true
		},
		"custom": {
			"variation": "default",
			"borderRadius": {
				"full": "999px",
				"large": "12px",
				"medium": "6px",
				"small": "4px",
				"xLarge": "18px"
			},
			"fontWeight": {
				"black": 900,
				"bold": 700,
				"extraBold": 800,
				"extraLight": 200,
				"light": 300,
				"medium": 500,
				"regular": 400,
				"semiBold": 600,
				"thin": 100
			},
			"lineHeight": {
				"initial": 1,
				"large": 1.8,
				"medium": 1.6000000000000001,
				"small": 1.5,
				"xSmall": 1.2
			}
		},
		"layout": {
			"contentSize": "1280px",
			"wideSize": "100%"
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"increment": 0,
				"mediumStep": 0,
				"operator": "*",
				"steps": 0,
				"unit": "px"
			},
			"spacingSizes": [
				{
					"name": "XX Small",
					"size": "calc(var(--wp--preset--font-size--medium,1rem)/2)",
					"slug": "xx-small"
				},
				{
					"name": "X Small",
					"size": "calc(var(--wp--preset--font-size--medium,1rem))",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "clamp(var(--wp--preset--font-size--medium), 4vw, calc(var(--wp--preset--font-size--medium,1rem)*1.5))",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(var(--wp--preset--font-size--medium), 4vw, calc(var(--wp--preset--font-size--medium,1rem)*2))",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "calc(var(--wp--preset--font-size--medium,1rem)*3)",
					"slug": "large"
				},
				{
					"name": "X Large",
					"size": "calc(var(--wp--preset--font-size--medium,1rem)*4)",
					"slug": "x-large"
				},
				{
					"name": "XX Large",
					"size": "calc(var(--wp--preset--font-size--medium,1rem)*5)",
					"slug": "xx-large"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"fontFamilies": [
				{
					"fontFamily": "Chesna Grotesk Regular",
					"slug": "chesna-grotesk-regular",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Regular",
							"fontWeight": "450",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-regular_normal_450.ttf"
							]
						},
						{
							"fontFamily": "Chesna Grotesk Regular",
							"fontWeight": "450",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/chesna-grotesk-regular_italic_450.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Chesna Grotesk Semi Bold",
					"slug": "chesna-grotesk-semi-bold",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Semi Bold",
							"fontWeight": "600",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-semi-bold_normal_600.ttf"
							]
						},
						{
							"fontFamily": "Chesna Grotesk Semi Bold",
							"fontWeight": "600",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/chesna-grotesk-semi-bold_italic_600.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Chesna Grotesk Thin",
					"slug": "chesna-grotesk-thin",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Thin",
							"fontWeight": "100",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-thin_normal_100.ttf"
							]
						},
						{
							"fontFamily": "Chesna Grotesk Thin",
							"fontWeight": "100",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/chesna-grotesk-thin_italic_100.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Chesna Grotesk Bold",
					"slug": "chesna-grotesk-bold",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Bold",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-bold_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Chesna Grotesk Bold",
							"fontWeight": "700",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/chesna-grotesk-bold_italic_700.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Chesna Grotesk Book",
					"slug": "chesna-grotesk-book",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Book",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-book_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Chesna Grotesk Book",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/chesna-grotesk-book_italic_400.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Chesna Grotesk Extra Bold",
					"slug": "chesna-grotesk-extra-bold",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Extra Bold",
							"fontWeight": "800",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-extra-bold_normal_800.ttf"
							]
						},
						{
							"fontFamily": "Chesna Grotesk Extra Bold",
							"fontWeight": "800",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/chesna-grotesk-extra-bold_italic_800.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Chesna Grotesk Extra Light",
					"slug": "chesna-grotesk-extra-light",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Extra Light",
							"fontWeight": "200",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/chesna-grotesk-extra-light_italic_200.ttf"
							]
						},
						{
							"fontFamily": "Chesna Grotesk Extra Light",
							"fontWeight": "200",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-extra-light_normal_200.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Chesna Grotesk Light",
					"slug": "chesna-grotesk-light",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Light",
							"fontWeight": "300",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-light_normal_300.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Chesna Grotesk Medium",
					"slug": "chesna-grotesk-medium",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Medium",
							"fontWeight": "500",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/chesna-grotesk-medium_italic_500.ttf"
							]
						},
						{
							"fontFamily": "Chesna Grotesk Medium",
							"fontWeight": "500",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-medium_normal_500.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Chesna Grotesk Black",
					"slug": "chesna-grotesk-black",
					"fontFace": [
						{
							"fontFamily": "Chesna Grotesk Black",
							"fontWeight": "900",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/chesna-grotesk-black_normal_900.ttf"
							]
						},
						{
							"fontFamily": "Chesna Grotesk Black",
							"fontWeight": "900",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/chesna-grotesk-black_italic_900.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "14px",
						"min": "12px"
					},
					"name": "X Small",
					"size": "clamp(12px, 1.5vw, 14px)",
					"slug": "x-small"
				},
				{
					"fluid": {
						"max": "16px",
						"min": "14px"
					},
					"name": "Small",
					"size": "clamp(14px, 1.6vw, 16px)",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "18px",
						"min": "16px"
					},
					"name": "Normal",
					"size": "clamp(16px, 1.8vw, 18px)",
					"slug": "normal"
				},
				{
					"fluid": {
						"max": "22px",
						"min": "20px"
					},
					"name": "Large",
					"size": "clamp(18px, 2.2vw, 20px)",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "28px",
						"min": "24px"
					},
					"name": "X Large",
					"size": "clamp(24px, 3vw, 28px)",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "36px",
						"min": "28px"
					},
					"name": "XX Large",
					"size": "clamp(28px, 3.6vw, 36px)",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "48px",
						"min": "36px"
					},
					"name": "XXX Large",
					"size": "clamp(36px, 4.8vw, 48px)",
					"slug": "xxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--text-primary)",
			"background": "var(--wp--preset--color--background-primary)"
		},
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-excerpt": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--x-small)",
						"bottom": "var(--wp--preset--spacing--x-small)"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "1.5rem"
				}
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "#0b8276"
				},
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			},
			"h1": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)"
				}
			},
			"h2": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--x-small)",
						"top": "var(--wp--preset--spacing--medium)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h3": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--x-small)",
						"top": "var(--wp--preset--spacing--medium)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--xx-small)",
						"top": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--xx-small)",
						"top": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h6": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--xx-small)",
						"top": "var(--wp--preset--spacing--x-small)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"heading": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--large)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--custom-links-hover)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--custom-links)"
				},
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--large)"
				}
			}
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"right": "var(--wp--preset--spacing--x-small)",
				"left": "var(--wp--preset--spacing--x-small)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibm-plex-sans)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontWeight": "400",
			"lineHeight": "var(--wp--custom--line-height--large)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"title": "Header",
			"name": "header"
		},
		{
			"name": "header-transparent",
			"title": "Header (Transparent)",
			"area": "header"
		},
		{
			"area": "footer",
			"title": "Footer",
			"name": "footer"
		}
	]
}